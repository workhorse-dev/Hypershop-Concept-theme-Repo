{% include 'shogun-content-handler' %}
<html class="no-js" lang="{{ request.locale.iso_code }}" dir="{{ direction }}">
  <head>

  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width,initial-scale=1">
  <meta name="theme-color" content="{{ settings.color_button_background }}">
  <meta name="google-site-verification" content="Jf-0DovDM7NdRpyzsWbXDMwkp73dHu7etk4XSzE93Gs" />
  
  {% if canonical_url contains 'page=' %}
    {%- assign url_parts = canonical_url | split: '?' -%}
    {%- assign root_url = url_parts[0] -%}
    <link rel="canonical" href="{{ root_url }}">
    <meta name="robots" content="noindex, follow"> 
  {% else %}
    <link rel="canonical" href="{{ canonical_url }}">
  {% endif %}
  <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>
  <link rel="preconnect" href="https://fonts.shopifycdn.com" crossorigin>
  {% # theme-check-disable RemoteAsset %}
  <link rel="dns-prefetch" href="https://ajax.googleapis.com">
  <link rel="dns-prefetch" href="https://maps.googleapis.com">
  <link rel="dns-prefetch" href="https://maps.gstatic.com">
  {% # theme-check-enable RemoteAsset %}
  <script src="https://www.google.com/recaptcha/api.js?render=6LfIQlorAAAAAEKuHgPFzXQMJvnwPP2_T0V4Kwdz" async></script>
  <script async type="module" fetchpriority="high" src="https://cdn.shopify.com/s/files/1/0872/6416/files/intelligems.js?v=1749207478"></script>
  {% comment %} <script src="https://cdn.shopify.com/s/files/1/0872/6416/files/widget.js?v=1749207346" data-account="2xDigTdBpS" defer></script> {% endcomment %}
  
  <script>
      (function() {
          const currentUrl = window.location.href;
          const urlPath = window.location.pathname;
          const lowercaseUrl = currentUrl.toLowerCase();
          if (urlPath !== urlPath.toLowerCase()) {
              window.location.replace(lowercaseUrl);
          }
      })();
   </script>
   {% render 'dynamic-redirects' %}

  
  {%- if settings.favicon != blank -%}
    <link rel="shortcut icon" href="{{ settings.favicon | image_url: width: 32, height: 32 }}" type="image/png" />
  {%- endif -%}

  <title>
    {{- page_title -}}
    {%- if current_tags -%}
      {%- assign meta_tags = current_tags | join: ', ' %} &ndash; {{ 'general.meta.tags' | t: tags: meta_tags -}}
    {%- endif -%}
    {%- if current_page != 1 %} &ndash; {{ 'general.meta.page' | t: page: current_page }}{% endif -%}
    {%- if request.page_type == 'password' -%}
      {{- shop.name -}}
    {%- else -%}
      {%- unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless -%}
    {%- endif -%}
  </title>

  {%- if page_description -%}
    <meta name="description" content="{{ page_description | escape }}">
  {%- endif -%}
  
  {%- liquid
    render 'social-meta-tags'
    
    render 'css-variables'
    
    echo 'theme.css' | asset_url | stylesheet_tag: preload: true
    echo 'style.css' | asset_url | stylesheet_tag: preload: true
    
    if direction == 'rtl'
      echo 'rtl.css' | asset_url | stylesheet_tag: preload: true
    endif
  -%}
  {% render 'custom-fonts' %}
  <link rel="stylesheet" href="{{ 'apps.css' | asset_url }}?v={{ 'now' | date: '%s' }}" media="print" fetchpriority="low" onload="this.media='all'">

  {%- if tinyscript -%}{{ content_for_header }}{%- else -%}{% render 'tiny-script-control' %}{%- endif -%}

  <script src="{{ 'vendor.js' | asset_url }}?v={{ 'now' | date: '%s' }}" defer="defer"></script>
  <script src="{{ 'theme.js' | asset_url }}?v={{ 'now' | date: '%s' }}" defer="defer"></script>

  {%- if settings.enable_tab_attention -%}
    <script src="{{ 'tab-attention.js' | asset_url }}" defer="defer"></script>
  {%- endif -%}

  {%- if request.page_type contains 'customers/' -%}
    <script src="{{ 'shopify_common.js' | shopify_asset_url }}?v={{ 'now' | date: '%s' }}" defer="defer"></script>
  {%- endif -%}

  {%- render 'js-variables' -%}

  {% if template contains '404' %}
      <script>window.location.href = "/";</script>
  {% endif %}

  
  {% render 'utm-store' %}
  {% render 'utm-append' %}

  {% render 'shogun-head' %}
</head>
  <body class="template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}{% if request.path == '/challenge' %} template-challange{% endif %}{% if request.design_mode %} shopify-design-mode{% endif %}" data-rounded-button="{{ settings.rounded_button }}" data-rounded-input="{{ settings.rounded_input }}" data-rounded-block="{{ settings.rounded_block }}" data-rounded-card="{{ settings.rounded_card }}" data-button-hover="{{ settings.buttons_hover }}" if settings.enable_lazy_image data-lazy-image endif data-title-animation data-page-rendering>
    {% include 'shogun-content-handler' %}

    
    
    {{ content_for_layout }}
    
    <script src="{{ 'instant-page.js' | asset_url }}" type="module" fetchpriority="low" defer></script>
<script>
    document.addEventListener("DOMContentLoaded", function () {
    if (window.location.search.includes("customer_posted=true")) {
      if (window.location.hash.includes("newsletter") || window.location.hash === "" ) {
        fbq('track', 'SubmitApplication');
        console.log("Meta Pixel: SubmitApplication event fired");
      }
    }
  });
  </script>
<script>
  window.addEventListener('load', function () {
    var badge = document.querySelector('.grecaptcha-badge');
    var footer = document.querySelector('.site-footer'); // Adjust this selector to your actual footer
    if (badge && footer && !footer.contains(badge)) {
      // Wrap it in a container to style it easily
      var wrapper = document.createElement('div');
      wrapper.className = 'recaptcha-badge-wrapper';
      wrapper.appendChild(badge);
      footer.appendChild(wrapper);
    }
  });
</script>
<script>
    (function(d){
      var s = d.createElement("script");
       s.setAttribute("data-position", 4);
       {% comment %} s.setAttribute("data-z-index", 10001); {% endcomment %}
      s.setAttribute("data-widget_layout", "full")
      s.setAttribute("data-account", "2xDigTdBpS");
      s.setAttribute("src", "https://cdn.userway.org/widget.js");
      (d.body || d.head).appendChild(s);
    })(document)
    </script>
<script>
      window.addEventListener("load", function () {
        setTimeout(function () {
          const uw = document.querySelector(".userway_buttons_wrapper");
          if (uw) {
            uw.style.setProperty("left", "30px", "important");
            uw.style.setProperty("bottom", "55px", "important");
            uw.style.setProperty("top", "unset", "important");
            uw.style.setProperty("transform", "unset", "important");
          }
        }, 2000);
      });
    </script>
    <style type="text/css">
      body, html {
        width: 100% !important;
        background: white !important;
      }
    </style>
    
  </body>
</html>
